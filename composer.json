{
    "name": "akaunting/laravel-language",
    "description": "Language switcher package for Laravel 11 and 12",
    "keywords": [
        "laravel",
        "language",
        "switcher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Duli√ßi",
            "email": "info@akaunting.com",
            "homepage": "https://akaunting.com",
            "role": "Developer"
        },
        {
            "name": "Daniel Esteban Arias (fork maintainer)",
            "email": "dariassoft@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0 || ^12.0",
        "jenssegers/agent": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\Language\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\Language\\Provider"
            ],
            "aliases": {
                "Language": "Akaunting\\Language\\Facade"
            }
        }
    }
}
